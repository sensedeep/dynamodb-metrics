{
    "name": "dynamodb-metrics",
    "version": "1.0.0",
    "description": "Instrument DynamoDB with Metrics for Single Table Designs",
    "main": "dist/cjs/index.js",
    "module": "dist/mjs/index.js",
    "types": "dist/mjs/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/mjs/index.js",
            "require": "./dist/cjs/index.js"
        }
    },
    "keywords": [
        "dynamodb",
        "metrics",
        "serverless",
        "log",
        "library",
        "aws"
    ],
    "author": {
        "name": "Michael O'Brien",
        "email": "mob@sensedeep.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/sensedeep/dynamodb-metrics.git"
    },
    "bugs": {
        "url": "http://github.com/sensedeep/dynamodb-metrics/issues"
    },
    "homepage": "https://github.com/sensedeep/dynamodb-metrics",
    "license": "MIT",
    "scripts": {
        "build": "rm -fr dist/* && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && ./fixup",
        "lint": "eslint src",
        "prepare": "npm run build",
        "_prepublishOnly": "npm test && npm run lint",
        "test": "jest",
        "test-cov": "jest --coverage"
    },
    "dependencies": {
        "@aws-sdk/client-dynamodb": "^3.30.0",
        "@aws-sdk/lib-dynamodb": "^3.30.0",
        "@aws-sdk/util-dynamodb": "^3.5.0"
    },
    "devDependencies": {
        "aws-sdk": "^2.718.0",
        "@types/jest": "^26.0.19",
        "@types/node": "^14.14.16",
        "dynamo-db-local": "^4.0.2",
        "eslint": "^7.25.0",
        "jest": "^26.6.3",
        "ts-jest": "^26.4.4",
        "typescript": "^4.1.3",
        "wait-port": "^0.2.9"
    },
    "files": [
        "dist/"
    ]
}
